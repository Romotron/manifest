<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="git://github.com" />

  <remote  name="private"
           fetch="ssh://git@github.com" />

  <remote  name="android"
           fetch="https://android.googlesource.com" />

  <remote  name="kernel"
           fetch="git://git.kernel.org" />

  <remote  name="busybox"
           fetch="git://git.busybox.net" />

  <remote  name="savannah"
           fetch="git://git.savannah.gnu.org" />

  <default revision="refs/heads/master"
           remote="github"
           sync-c="true"
           sync-j="4" />

  <project path="build" name="efidroid/build" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- target compilers -->
  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" name="platform/prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" remote="android" revision="refs/tags/android-5.1.0_r3" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-architoolchain-linux-gnueabihf-5.2" name="ArchiDroid/Toolchain" revision="architoolchain-5.2-arm-linux-gnueabihf" />

  <!-- modules -->
  <project path="modules/mkbootimg" name="efidroid/modules_mkbootimg" />
  <project path="modules/multiboot" name="efidroid/modules_multiboot" />
  <project path="modules/tracy" name="efidroid/modules_tracy" />
  <project path="modules/util-linux" name="pub/scm/utils/util-linux/util-linux" remote="kernel" />
  <project path="modules/busybox" name="busybox" remote="busybox" />
  <project path="modules/selinux" name="SELinuxProject/selinux" />
  <project path="modules/inih" name="benhoyt/inih" />
  <project path="modules/libaroma" name="amarullz/libaroma" />
  <project path="modules/e2fsprogs" name="pub/scm/fs/ext2/e2fsprogs" remote="kernel" revision="refs/tags/v1.42.13" />
  <project path="modules/f2fs-tools" name="pub/scm/linux/kernel/git/jaegeuk/f2fs-tools" remote="kernel" revision="refs/tags/v1.6.0" />
  <project path="modules/libfuse" name="libfuse/libfuse" />

  <!-- UEFI -->
  <project path="uefi/edk2" name="efidroid/edk2" />
  <project path="uefi/edk2packages/LittleKernelPkg" name="efidroid/uefi_edk2packages_LittleKernelPkg" />
  <project path="uefi/edk2packages/GrubFSPkg" name="efidroid/uefi_edk2packages_GrubFSPkg" />
  <project path="uefi/lkmodules/uefiapi" name="efidroid/uefi_lkmodules_uefiapi" />

  <!-- LK -->
  <project path="bootloader/lk/common/LA.BF64" name="efidroid/bootloader_lk" revision="common/LA.BF64" />

  <!-- apps -->
  <project path="uefi/apps/EFIDroidUi" name="efidroid/uefi_apps_EFIDroidUi" />

</manifest>
